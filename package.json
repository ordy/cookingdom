{
	"name": "cookingdom",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"config": "ts-node ./src/assets/scripts/setEnv.ts",
		"start": "npm run config -- --environment=dev && ng serve",
		"build": "npm run config -- --environment=prod && ng build --prod",
		"build:stats": "ng build --stats-json",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"analyze": "webpack-bundle-analyzer dist/cookingdom/stats.json",
		"vercel-build": "npm run config -- --environment=prod && ng build --prod --aot --build-optimizer"
	},
	"engines": {
		"node": "14.15.3",
		"npm": "6.14.11"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^11.1.1",
		"@angular/common": "^11.1.1",
		"@angular/core": "^11.1.1",
		"@angular/fire": "^6.1.4",
		"@angular/forms": "^11.1.1",
		"@angular/localize": "^11.1.1",
		"@angular/platform-browser": "^11.1.1",
		"@angular/platform-browser-dynamic": "^11.1.1",
		"@angular/router": "^11.1.1",
		"@fortawesome/angular-fontawesome": "^0.8.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@fortawesome/free-brands-svg-icons": "^5.15.2",
		"@fortawesome/free-solid-svg-icons": "^5.15.2",
		"@ng-bootstrap/ng-bootstrap": "^9.0.2",
		"bootstrap": "^4.6.0",
		"firebase": "^7.24.0",
		"rxjs": "^6.6.3",
		"tslib": "^2.1.0",
		"zone.js": "0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1101.2",
		"@angular/cli": "^11.1.2",
		"@angular/compiler": "^11.1.1",
		"@angular/compiler-cli": "^11.1.1",
		"@angular/language-service": "^11.1.1",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "^2.0.8",
		"@types/node": "^14.14.22",
		"codelyzer": "^6.0.0",
		"dotenv": "~8.2.0",
		"jasmine": "3.5.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "^5.2.3",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "^3.0.3",
		"karma-firefox-launcher": "^1.3.0",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"ts-node": "8.10.2",
		"tslint": "^6.1.3",
		"typescript": "~4.1.5",
		"webpack-bundle-analyzer": "^3.9.0"
	}
}
